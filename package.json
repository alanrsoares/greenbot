{
  "name": "@greenbot/cli",
  "version": "0.18.2",
  "keywords": [
    "greenbot",
    "cli",
    "package updater"
  ],
  "description": "An interactive package updater for npm based applications",
  "homepage": "https://github.com/alanrsoares/greenbot",
  "license": "MIT",
  "files": [
    "dist/",
    "bin/"
  ],
  "bin": "./bin/greenbot.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "preversion": "yarn build",
    "release": "npm publish && git push && git push --tags"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.49",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.7",
    "broadcast-channel": "^4.13.0",
    "cssnano": "^5.1.12",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "svelte": "^3.48.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tailwindcss": "^3.1.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "vite": "^2.9.13"
  },
  "dependencies": {
    "@sveltestack/svelte-query": "^1.6.0",
    "body-parser": "^1.20.0",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "ky": "^0.31.0",
    "open": "^8.4.0",
    "package-json": "^7.0.0",
    "rambda": "^7.1.4",
    "replace-in-file": "^6.3.5",
    "svelte-icons": "^2.1.0"
  }
}
