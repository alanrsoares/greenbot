{
  "name": "@greenbot/cli",
  "version": "0.26.9",
  "keywords": [
    "greenbot",
    "cli",
    "package updater"
  ],
  "description": "An interactive package updater for npm based applications",
  "homepage": "https://github.com/alanrsoares/greenbot",
  "license": "MIT",
  "files": [
    "dist/",
    "bin/",
    "scripts/"
  ],
  "bin": "./bin/greenbot.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "preversion": "yarn build",
    "release": "npm publish && git push && git push --tags",
    "prepare": "husky install",
    "postinstall": "node scripts/postinstall.cjs"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.2",
    "@tsconfig/strictest": "^2.0.1",
    "@types/cors": "^2.8.13",
    "autoprefixer": "^10.4.14",
    "chalk": "^4.1.2",
    "cssnano": "^6.0.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.26",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "strip-ansi": "^6.0.1",
    "svelte-check": "^3.4.6",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.5",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/svelte-query": "^4.29.25",
    "animate.css": "^4.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "daisyui": "^3.3.1",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "ky": "^0.33.3",
    "lucide-svelte": "^0.244.0",
    "open": "^8.4.2",
    "package-json": "^7.0.0",
    "rambda": "^7.5.0",
    "replace-in-file": "^7.0.1",
    "svelte": "^4.1.1",
    "svelte-markdown": "^0.2.3",
    "typewriter-effect": "^2.20.1"
  }
}
