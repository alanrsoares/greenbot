{
  "name": "@greenbot/cli",
  "version": "0.29.3",
  "keywords": [
    "greenbot",
    "cli",
    "package updater"
  ],
  "description": "An interactive package updater for npm based applications",
  "homepage": "https://github.com/alanrsoares/greenbot",
  "license": "MIT",
  "files": [
    "dist/",
    "bin/",
    "scripts/"
  ],
  "bin": "./bin/greenbot.mjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'node bin/greenbot.mjs'",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postversion": "pnpm build && git add . && git commit --amend --no-edit",
    "release": "pnpm publish && git push && git push --tags",
    "prepare": "husky install",
    "prebuild": "node scripts/prebuild.mjs",
    "postinstall": "node scripts/postinstall.mjs"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.4.6",
    "@tsconfig/strictest": "^2.0.2",
    "@types/cors": "^2.8.14",
    "autoprefixer": "^10.4.16",
    "chalk": "^4.1.2",
    "concurrently": "^8.2.1",
    "cssnano": "^6.0.1",
    "husky": "^8.0.3",
    "postcss": "^8.4.30",
    "postcss-load-config": "^4.0.1",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "strip-ansi": "^6.0.1",
    "svelte-check": "^3.5.2",
    "svelte-preprocess": "^5.0.4",
    "tailwindcss": "^3.3.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-tsconfig-paths": "^4.2.1",
    "zx": "^7.2.3"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/svelte-query": "^4.35.3",
    "animate.css": "^4.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "daisyui": "^3.7.6",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "ky": "^0.33.3",
    "lucide-svelte": "^0.244.0",
    "open": "^8.4.2",
    "package-json": "^8.1.1",
    "rambda": "^7.5.0",
    "replace-in-file": "^7.0.1",
    "svelte": "^4.2.1",
    "svelte-markdown": "^0.2.3",
    "typewriter-effect": "^2.21.0"
  }
}
