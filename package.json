{
  "name": "@greenbot/cli",
  "version": "0.34.2",
  "packageManager": "bun@1.3.4",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "keywords": [
    "greenbot",
    "cli",
    "package updater"
  ],
  "description": "An interactive package updater for npm based applications",
  "homepage": "https://github.com/alanrsoares/greenbot",
  "license": "MIT",
  "files": [
    "dist/",
    "bin/",
    "scripts/"
  ],
  "bin": "./bin/greenbot.mjs",
  "type": "module",
  "scripts": {
    "dev": "concurrently 'vite' 'node bin/greenbot.mjs'",
    "build": "vite build",
    "serve": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "postversion": "bun run build && git add . && git commit --amend --no-edit",
    "release": "bun publish && git push && git push --tags",
    "prepare": "husky install",
    "prebuild": "node scripts/prebuild.mjs",
    "postinstall": "node scripts/postinstall.mjs"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@tsconfig/strictest": "^2.0.8",
    "@types/cors": "^2.8.19",
    "autoprefixer": "^10.4.22",
    "chalk": "^4.1.2",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.2",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.7.4",
    "pretty-quick": "^4.2.2",
    "strip-ansi": "^6.0.1",
    "svelte-check": "^4.3.4",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^4.1.18",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^6.4.1",
    "vite-tsconfig-paths": "^5.1.4",
    "zx": "^8.8.5"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/svelte-query": "^5.90.2",
    "animate.css": "^4.1.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/static": "^8.3.0",
    "daisyui": "^5.5.13",
    "fastify": "^5.6.2",
    "js-yaml": "^4.1.1",
    "ky": "^1.14.1",
    "lucide-svelte": "^0.488.0",
    "open": "^10.2.0",
    "package-json": "^10.0.1",
    "rambda": "^9.4.2",
    "replace-in-file": "^7.2.0",
    "svelte": "^4.2.20",
    "svelte-markdown": "^0.4.1",
    "typewriter-effect": "^2.22.0"
  }
}
